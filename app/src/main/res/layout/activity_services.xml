<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00EBF2F3"
    tools:context=".ServicesActivity">

    <ImageView
        android:id="@+id/imgMakeup"
        android:layout_width="269dp"
        android:layout_height="152dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="65dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnMakeup"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/makeup" />

    <Button
        android:id="@+id/btnMakeup"
        android:layout_width="138dp"
        android:layout_height="166dp"
        android:layout_marginTop="65dp"
        android:backgroundTint="#B1E6ED"
        android:text="MakeUp"
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgMassage"
        android:layout_width="259dp"
        android:layout_height="169dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnMassage"
        app:layout_constraintTop_toBottomOf="@+id/imgMakeup"
        app:srcCompat="@drawable/massage" />

    <Button
        android:id="@+id/btnMassage"
        android:layout_width="139dp"
        android:layout_height="177dp"
        android:layout_marginTop="16dp"
        android:backgroundTint="#3100BCD4"
        android:text="Massage"
        android:textColor="#131313"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMakeup" />

    <ImageView
        android:id="@+id/imgHairCare"
        android:layout_width="267dp"
        android:layout_height="146dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/btnHairCare"
        app:layout_constraintTop_toBottomOf="@+id/imgMassage"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ajs" />

    <Button
        android:id="@+id/btnHairCare"
        android:layout_width="130dp"
        android:layout_height="144dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="55dp"
        android:backgroundTint="#00BCD4"
        android:text="HairCare"
        android:textColor="#020202"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMassage"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgba"
        android:layout_width="39dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="328dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/btnMakeup"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />
</androidx.constraintlayout.widget.ConstraintLayout>